{
  "name": "express-posthtml",
  "version": "1.0.4",
  "description": "PostHTML View Engine for Express",
  "engines": { "node": ">=4" },
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "jest --verbose --coverage",
    "docs": "jsdoc2md index.js > INDEX.md",
    "clean": "rm -rf jest coverage jsdoc-api dmd",
    "start": "sudo npm run clean && npm run lint && npm test",
    "release": "standard-version"
  },
  "dependencies": {
    "posthtml": "^0.9.2"
  },
  "devDependencies": {
    "coveralls": "^2.12.0",
    "express": "^4.15.2",
    "jest": "^19.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "posthtml-bem": "^0.2.2",
    "posthtml-expressions": "^1.1.0",
    "standard": "^9.0.1",
    "standard-version": "^4.0.0",
    "supertest": "^3.0.0"
  },
  "standard": {
    "env": {
      "jest": true
    }
  },
  "keywords": [
    "HTML",
    "PostHTML",
    "Express",
    "View Engine"
  ],
  "author": {
    "name": "Michael Ciniawsky",
    "email": "<michael.ciniawsky@gmail.com>"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posthtml/express-posthtml.git"
  },
  "bugs": {
    "url": "https://github.com/posthtml/express-posthtml/issues"
  },
  "homepage": "https://github.com/posthtml/express-posthtml#readme",
  "license": "MIT"
}
